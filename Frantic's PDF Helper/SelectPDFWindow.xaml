<Window x:Class="Frantics_PDF_Helper.SelectPDFWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:Frantics_PDF_Helper"
		mc:Ignorable="d"
        
		Title="SelectPDFWindow" 
        Height="450"
        Width="800"
        WindowStartupLocation="CenterScreen"
		ResizeMode="NoResize">
    <Grid>
        <Button x:Name="selectPDFButton" Content="LoadPDFFile" Tag="LocKey:'SelectPDFWindow.LoadPDFFile'" HorizontalAlignment="Center" Margin="0,386,0,0" VerticalAlignment="Top" Height="38" Width="310"/>

    </Grid>
</Window>
